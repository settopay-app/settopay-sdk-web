{
  "name": "@setto-pay/external-sdk-web",
  "version": "0.1.4",
  "description": "Setto External SDK for Web - Integrate Setto payments into your web application",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "example": "npx serve .",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "setto",
    "payment",
    "sdk",
    "crypto",
    "stablecoin"
  ],
  "author": "Setto",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.2"
  }
}
